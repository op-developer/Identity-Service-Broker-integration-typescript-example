{
  "name": "demo-service-provider",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "start": "node ./dist/index.js",
    "build": "npm-run-all build:*",
    "build:ts": "tsc",
    "build:templates": "cp -r src/templates dist/templates",
    "build:locales": "cp -r src/locales dist/locales",
    "build:scss": "node-sass src/scss/style.scss dist/public/style.css --include-path node_modules",
    "develop": "npm-run-all --parallel develop:*",
    "develop:ts": "nodemon --ext ts,hbs --watch src --exec ts-node -- ./src/index.ts",
    "develop:scss": "npm run build:scss && npm run build:scss -- --watch",
    "develop:locales": "nodemon --ext json --watch src/locales --exec npm run -- build:locales"
  },
  "dependencies": {
    "@hapi/hapi": "^21.3.2",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "7.0.3",
    "@hapi/yar": "11.0.1",
    "@types/hapi__hapi": "^21.0.0",
    "@types/hapi__inert": "5.2.6",
    "@types/hapi__vision": "5.5.4",
    "@types/hapi__yar": "10.1.2",
    "@types/i18next-node-fs-backend": "2.1.1",
    "@types/node": "18.14.0",
    "@types/uuid": "9.0.0",
    "checkout-logger": "github:Heikkohoo/checkout-logger#7f8efd92db9ff8cf535286c0850488c5524fbe3f",
    "crypto-random-string": "^3.3.1",
    "handlebars": "^4.7.8",
    "i18next": "^23.5.1",
    "i18next-fs-backend": "2.2.0",
    "joi": "17.11.0",
    "node-jose": "^2.2.0",
    "node-sass": "^9.0.0",
    "nodemon": "3.0.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-plugin-functional": "^6.0.0"
  }
}
